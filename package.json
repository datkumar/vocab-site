{
  "name": "vocab-site",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "init-words": "tsx --env-file=.env src/init/init-script.ts",
    "db-clean": "tsx temp/data/db-data-cleaner.ts",
    "dev": "next dev --turbopack",
    "dev-local": "echo \"Local IP: $(hostname -I)\" && next dev -H 0.0.0.0",
    "pswd-hash": "tsx --env-file=.env src/lib/password-hasher.ts",
    "build": "next build",
    "start": "next start",
    "lint": "next lint"
  },
  "dependencies": {
    "@auth/mongodb-adapter": "^3.10.0",
    "@radix-ui/react-icons": "^1.3.2",
    "@tailwindcss/postcss": "^4.1.11",
    "axios": "^1.11.0",
    "bcrypt": "^6.0.0",
    "class-variance-authority": "^0.7.1",
    "clsx": "^2.1.1",
    "geist": "^1.4.2",
    "mongodb": "^6.18.0",
    "next": "15.4.5",
    "next-auth": "5.0.0-beta.25",
    "react": "19.1.1",
    "react-dom": "19.1.1",
    "tailwind-merge": "^3.3.1",
    "tailwindcss": "^4.1.11",
    "tailwindcss-animate": "^1.0.7",
    "tsx": "^4.20.3",
    "zod": "^4.0.15"
  },
  "devDependencies": {
    "@types/bcrypt": "^6.0.0",
    "@types/node": "^24.2.0",
    "@types/react": "19.1.9",
    "@types/react-dom": "19.1.7",
    "eslint": "^9.32.0",
    "eslint-config-next": "15.4.5",
    "postcss": "^8.5.6",
    "typescript": "^5.9.2"
  },
  "pnpm": {
    "overrides": {
      "@types/react": "19.0.2",
      "@types/react-dom": "19.0.2"
    },
    "onlyBuiltDependencies": [
      "bcrypt",
      "esbuild",
      "sharp",
      "unrs-resolver"
    ]
  }
}
